
config wifi-device 'radio0'
	option type 'mac80211'
	option path 'platform/10300000.wmac'
	option channel '1'
	option band '2g'
	option htmode 'HT20'
	option cell_density '0'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option encryption 'psk2'
	option key 'V!v!nt100'
	option ssid 'CHAMBER-45-2.4GHZ'

config wifi-device 'radio1'
	option type 'mac80211'
	option path 'pci0000:00/0000:00:00.0/0000:01:00.0'
	option channel '36'
	option band '5g'
	option htmode 'VHT80'
	option cell_density '0'

config wifi-iface 'default_radio1'
	option device 'radio1'
	option network 'lan'
	option mode 'ap'
	option encryption 'psk2'
	option key 'V!v!nt100'
	option ssid 'CHAMBER-45-5GHZ'

config wifi-iface 'bak'
	option device 'radio1'
	option network 'wifi'
	option mode 'ap'
	option encryption 'psk2'
	option key 'V!v!nt100'
	option ssid 'CHAMBER-45-BAK-5d03'

